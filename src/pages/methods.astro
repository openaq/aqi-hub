---
import PageLayout from "@layouts/PageLayout.astro";
import { getEntry } from "astro:content";
import { render } from "astro:content";

const methods = await getEntry("pages", "methods");
if (!methods) {
  throw new Error("Could not find methods");
}

const { Content } = await render(methods);
---

<style is:global>
  .lollipop-contentration {
    display: flex;
    flex-direction: row;
    margin: 20px 100px;
  }

</style>

<PageLayout>
  <Content />
</PageLayout>
